.box {
	
	/*correct the overflow to make gradients visible, then remove the right round border from the button, remove the right border*/
	overflow:visible;
	border-bottom-right-radius: 0px !important;
    border-top-right-radius: 0px !important;
	//border-right: 0px solid red !important;
	//border-right: 1px solid #1AB394;
	margin-right: 2.3125em !important;
    margin-left: .3125em !important;
	
	text-shadow:none !important;
	border-right: 0px !important;
	//border-width: 1px;
    border-style: solid;
	border-top-left-radius: .3125em;
	border-bottom-left-radius: .3125em;
	background-color: #ffffff;
    border-color: #00679C;
    color: #00679C;
    text-shadow: 0 1px 0 #eeeeee;
	
	min-width:50px;
 
  color: #00679C;
  margin-bottom: 20px;
  padding: 10px;
  position: relative;
}
 
.box:before {
	border:1.5px solid #1AB394;
  width: 4.17em;
  height: 4.17em;
  position: absolute;
  left: 100%;
  content: "";
  z-index:-1;
  
  -webkit-transform: translate(-50%,0%) scale(0.5,0.7071) rotate(45deg);
  transform: translate(-50%,0%) scale(0.5,0.7071) rotate(45deg);
  border-radius:0px;
  border-top-right-radius: 5px;
      background-color: #ffffff;
  
}
 .box:after{

 
 }
.box:before {
box-sizing: border-box;
  top: -1px;
 // background: linear-gradient(to right top, #ffffff 48%, #00679C 49%,#00679C 51%, rgba(0, 0, 0, 0) 52%);
}

.box:hover {
background-color: #00679C;
    border-color: #00679C;
    color: #ffffff;
}
.box:hover:before {
  background-color: #00679C;
    border-color: #00679C;
    color: #ffffff;
}


.box:active:before {
   background-color: #00679C;
    border-color: #00679C;
    color: #ffffff;
}


.box.ui-state-disabled {
    border-color: #999999 !important;
    color: #999999 !important;
    opacity: 1;
    cursor: default !important;
    pointer-events: none;
}


.box.ui-state-disabled:before {
   background-color: #ffffff;
    border-color: #999999;
    color: #ffffff;
  
}


